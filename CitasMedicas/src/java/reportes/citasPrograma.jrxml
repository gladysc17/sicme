	<queryString>
		<![CDATA[SELECT count (*), programa_academico,
     ( SELECT count(*) FROM cita WHERE estado = 'inasistido' and fecha_cita BETWEEN '01-01-2017' and  '27-08-2017' ) inasistido ,
     ( SELECT count(*) FROM cita WHERE estado = 'atendido' and fecha_cita BETWEEN '01-01-2017' and '27-08-2017' ) atendido ,
     ( SELECT count(*) FROM cita WHERE estado = 'pendiente' and fecha_cita BETWEEN '01-01-2017' and '27-08-2017' ) pendiente

FROM estudiante e
INNER JOIN cita c on c.id_usuario = e.identificacion_usuario
GROUP BY e.programa_academico]]>
	</queryString>
