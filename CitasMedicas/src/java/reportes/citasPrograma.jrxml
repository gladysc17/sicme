<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports http://jasperreports.sourceforge.net/xsd/jasperreport.xsd" name="report name" pageWidth="595" pageHeight="842" columnWidth="535" leftMargin="20" rightMargin="20" topMargin="20" bottomMargin="20" uuid="afabcec6-f2af-4de4-8649-9640d94722c3">
	<property name="ireport.zoom" value="1.0"/>
	<property name="ireport.x" value="0"/>
	<property name="ireport.y" value="0"/>
	<parameter name="fechaI" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="fechaF" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select distinct e.programa_academico,
 
(select count(c.servicio_cita) from cita c, estudiante e1 where c.id_usuario = e1.identificacion_usuario and c.servicio_cita = 'psicologia' 
and e1.programa_academico = e.programa_academico and fecha_cita BETWEEN $P{fechaI} and $P{fechaF}) "Psicología",

(select count(c.servicio_cita) from cita c, estudiante e1 where c.id_usuario = e1.identificacion_usuario and c.servicio_cita = 'medicinageneral' 
and e1.programa_academico = e.programa_academico and fecha_cita BETWEEN $P{fechaI} and $P{fechaF}) "Medicina general",

(select count(c.servicio_cita) from cita c, estudiante e1 where c.id_usuario = e1.identificacion_usuario and c.servicio_cita = 'odontologia' 
and e1.programa_academico = e.programa_academico and fecha_cita BETWEEN $P{fechaI} and $P{fechaF}) "Odontología",

(select count(c.servicio_cita) from cita c, estudiante e1 where c.id_usuario = e1.identificacion_usuario and c.servicio_cita = 'planificacionfamiliar' 
and e1.programa_academico = e.programa_academico and fecha_cita BETWEEN $P{fechaI} and $P{fechaF}) "Planificación familiar",

(select count(c.servicio_cita) from cita c, estudiante e1 where c.id_usuario = e1.identificacion_usuario and e1.programa_academico = e.programa_academico
and fecha_cita BETWEEN $P{fechaI} and $P{fechaF}) "Total"

from estudiante e
ORDER BY e.programa_academico]]>
	</queryString>
	<field name="programa_academico" class="java.lang.String"/>
	<field name="Psicología" class="java.lang.Long"/>
	<field name="Medicina general" class="java.lang.Long"/>
	<field name="Odontología" class="java.lang.Long"/>
	<field name="Planificación familiar" class="java.lang.Long"/>
	<field name="Total" class="java.lang.Long"/>
