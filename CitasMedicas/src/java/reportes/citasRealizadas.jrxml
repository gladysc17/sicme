	<queryString>
		<![CDATA[SELECT
     count(*) total,
     ( SELECT count(*) FROM cita WHERE estado = 'inasistido' and fecha_cita BETWEEN $P{fechaI} and  $P{fechaF} ) inasistido ,
     ( SELECT count(*) FROM cita WHERE estado = 'atendido' and fecha_cita BETWEEN $P{fechaI} and $P{fechaF} ) atendido ,
     ( SELECT count(*) FROM cita WHERE estado = 'pendiente' and fecha_cita BETWEEN $P{fechaI} and $P{fechaF} ) pendiente
